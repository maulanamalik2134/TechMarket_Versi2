<?xml version="1.0" encoding="UTF-8"?>
<GridPane fx:id="root" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.MyController">
   <children>
      <Label text="Lokasi" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.columnSpan="2" font={{name: "Microsoft Sans Serif", size: 18}}/>
      <Label text="Tanggal" GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.columnSpan="2" font={{name: "Microsoft Sans Serif", size: 18}}/>
      <Label text="FORGOT" GridPane.columnIndex="1" GridPane.rowIndex="3" font={{name: "Microsoft Sans Serif", size: 36}}/>
      <Label text="Username" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
      <TextField fx:id="txt_username" GridPane.columnIndex="1" GridPane.rowIndex="4" GridPane.columnSpan="2" HBox.hgrow="ALWAYS" font={{name: "Times New Roman", size: 18}} onAction="#txt_usernameActionPerformed"/>
      <PasswordField fx:id="txt_password" GridPane.columnIndex="1" GridPane.rowIndex="5" GridPane.columnSpan="2" HBox.hgrow="ALWAYS" font={{name: "Microsoft Sans Serif", size: 18}} onAction="#txt_passwordActionPerformed"/>
      <PasswordField fx:id="txt_konfirmasipassword" GridPane.columnIndex="1" GridPane.rowIndex="6" GridPane.columnSpan="2" HBox.hgrow="ALWAYS" font={{name: "Microsoft Sans Serif", size: 18}} onAction="#txt_konfirmasipasswordActionPerformed"/>
      <CheckBox fx:id="chk_showpassword" GridPane.columnIndex="2" GridPane.rowIndex="6" text="Show Password" HBox.hgrow="ALWAYS" onAction="#chk_showpasswordActionPerformed"/>
      <Label text="Password" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
      <Label text="Konfirmasi Password" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
      <Button fx:id="btn_cancel" text="CANCEL" GridPane.columnIndex="1" GridPane.rowIndex="7" font={{name: "Microsoft Sans Serif", size: 24}} onAction="#btn_cancelActionPerformed"/>
      <Button fx:id="btn_forgot" text="FORGOT" GridPane.columnIndex="2" GridPane.rowIndex="7" font={{name: "Microsoft Sans Serif", size: 24}} onAction="#btn_forgotActionPerformed"/>
   </children>
</GridPane>
